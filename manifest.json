{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "1.2.0",
  "author": "YUKI \"Piro\" Hiroshi",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "activeTab",
    "downloads",
    "menus",
    "notifications",
    "storage",
    "tabs"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "resources/Save.svg#default",
    "20": "resources/Save.svg#default",
    "24": "resources/Save.svg#default",
    "32": "resources/Save.svg#default"
  },
  "background": {
    "page": "background/background.html"
  },
  "commands": {
    "saveSelectedTabs": {
      "description": "__MSG_command_saveSelectedTabs__"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "save-selected-tabs-to-files@piro.sakura.ne.jp",
      "strict_min_version": "67.0"
    }
  }
